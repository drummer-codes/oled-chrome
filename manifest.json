{
  "name": "OLED Black",
  "version": "1.0",
  "description": "True Black Theme for Chrome Optimized for OLED Screens",
  "manifest_version": 2,
  "theme": {
      "images": {
          "theme_frame": "frame.png",
          "theme_toolbar": "toolbar.png",
          "theme_tab_background": "tab_background.png"
      },
      "colors": {
          "frame": [ 0, 0, 0 ],
          "toolbar": [ 0, 0, 0 ],
          "tab_text": [ 250, 250, 250 ],
          "tab_background_text": [ 200, 200, 200 ],
          "bookmark_text": [ 200, 200, 200 ],
          "ntp_background": [ 0, 0, 0 ],
          "ntp_text": [ 100, 100, 100 ],
          "ntp_link": [ 100, 100, 100 ],
          "button_background": [ 0, 0, 0, 1 ]
      },
      "tints": {
          "buttons": [ 0, 0, 0.5 ]
      },
      "properties": {
          "ntp_background_alignment": "bottom",
          "ntp_background_repeat": "no-repeat"
      }
  }
}